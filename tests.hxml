--each
    -main RunTests
    -cp src
    -cp test
    -resource test/resources/snapshot-rev4.json@snapshot-rev4

--next
    -js bin/js/Main.js
    -cmd echo "Target: JS"
    -cmd node bin/js/Main.js
    -cmd echo "- - - - - - - - - - "

--next
    -php bin/php
    -cmd echo "Target: PHP"
    -cmd echo "- - - - - - - - - - "
    -cmd php bin/php/index.php

--next
    -cpp bin/cpp
    -cmd echo "Target: CPP"
    -cmd bin/cpp/RunTests
    -cmd echo "- - - - - - - - - - "

--next
    -java bin/java
    -cmd echo "Target: JAVA"
    -cmd java -jar bin/java/RunTests.jar
    -cmd echo "- - - - - - - - - - "

--next
    -python bin/python/main.py
    -cmd echo "Target: PYTHON"
    -cmd python bin/python/main.py
    -cmd echo "- - - - - - - - - - "

--next
    # Somehow, using --next doesn't work on my computer with C#.
    -cs bin/cs
    -main RunTests
    -cp src
    -cp test
    -resource test/resources/snapshot-rev4.json@snapshot-rev4
    -cmd echo "Target: C#"
    -cmd mono bin/cs/bin/RunTests.exe
    -cmd echo "- - - - - - - - - - "

--next
    #Neither with Lua
    -lua bin/lua/main.lua
    -main RunTests
    -cp src
    -cp test
    -resource test/resources/snapshot-rev4.json@snapshot-rev4
    -cmd echo "Target: Lua"
    -cmd lua bin/lua/main.lua
    -cmd echo "- - - - - - - - - - "
